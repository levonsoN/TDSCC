      <ComboBox fx:id="regionBox" layoutX="97.0" layoutY="109.0" prefHeight="25.0" prefWidth="168.0" />
      <Spinner fx:id="tempBox" editable="true" layoutX="97.0" layoutY="16.0" prefHeight="25.0" prefWidth="168.0" />
      <ComboBox fx:id="precipBox" layoutX="97.0" layoutY="47.0" prefHeight="25.0" prefWidth="168.0" />
      <Label layoutX="13.0" layoutY="20.0" prefHeight="17.0" prefWidth="79.0" text="Temperature:" />
      <Label layoutX="15.0" layoutY="51.0" text="Precipitation:" />
      <Label layoutX="15.0" layoutY="82.0" prefHeight="17.0" prefWidth="70.0" text="Date:" />
      <Label layoutX="15.0" layoutY="113.0" prefHeight="17.0" prefWidth="70.0" text="Region:" />
</AnchorPane>
